{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  },
  "extensions": {
    "queues": {
        "visibilityTimeout" : "00:00:15",
        "maxDequeueCount": 7
    },
    "blobs": {
      "maxDequeueCount": 3
    }
  }
}